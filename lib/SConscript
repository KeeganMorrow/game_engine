# Import the environment configured in SConstruct
Import('env')

localenv = env.Clone()

#Source files in this directory
engineobjects = ['main.cpp']

#TODO: Seperate this out from other library environments
localenv.Append(CPPPATH=['gtest-1.7.0/include'])
localenv.Append(CPPPATH=['gtest-1.7.0/'])

# Executable Binaries
localenv.StaticLibrary('gtest', 'gtest-1.7.0/src/gtest-all.cc', LIBS='pthreads', LIBPATH='.', RPATH=env.Literal('\\$$ORIGIN'))
